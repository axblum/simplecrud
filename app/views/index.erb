<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Hello and Welcome to my test project</title>
	</head>
	<body>
		<h1>Guest List</h1>
		<table style="width:100%">
		<% @attributes.each_key do |k| %>
		<tr><%= k %>
		</tr>
		<% end %>
		<% @guests.each do |guest| %>
		<tr>
		<% guest.attributes.each do |k,v| %>
		<td> <%= v %></td>
		<% end %>
		</tr>
		<% end %>
		</table>
		<form action="/" method="POST">
			<input type="text" name="textbox">
			<input type="submit">
		</form>
	</body>
</html>